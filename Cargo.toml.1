[package]
name = "gaddag-rust"
version = "0.1.0"
authors = ["Simon Chervenak <sarras305@gmail.com>"]
edition = "2018"

# See more keys and their definitions at https://doc.rust-lang.org/cargo/reference/manifest.html

[dependencies]
rand = "0.8.4"
itertools = "0.10.1"
array-init = "2.0.0"
petgraph = {version = "0.6.0", features = ["serde-1"]}
indicatif = "0.16.2"
gtk = "0.14.3"
gdk = "0.14.3" 
relm = "0.22.0"
relm-derive = "0.22.0"
rayon = "1.4.0"
clap = {version = "2.33.3", features = ["yaml", "color"]}
serde = {version = "1.0.116", features = ["derive"]}
bincode = "1.3.1"
glib = "0.14.8"
base64 = "0.13.0"
termion = "1.5.5"